{
  "name": "run-gulp-task",
  "version": "0.2.0",
  "description": "Run gulp task with Promise API",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test _mocha --require should --reporter spec",
    "test-cov": "cross-env NODE_ENV=test istanbul cover -x ./test/source/**/* ./node_modules/mocha/bin/_mocha -- --require should",
    "test-travis": "cross-env NODE_ENV=test istanbul cover -x ./test/source/**/* ./node_modules/mocha/bin/_mocha --report lcovonly -- --require should"
  },
  "author": "LingyuCoder",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.1.3",
    "gulp": "~3.9.0",
    "istanbul": "~0.4.0",
    "mocha": "~5.0.0",
    "plugin-error": "^1.0.1",
    "should": "~13.2.1",
    "through2": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ly-tools/run-gulp-task"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "keywords": [
    "gulp",
    "run-gulp-task"
  ],
  "bugs": {
    "url": "https://github.com/ly-tools/run-gulp-task/issues"
  },
  "dependencies": {
    "co": "~4.6.0",
    "get-gulp-inst": "0.0.2",
    "lodash": "~4.17.4",
    "run-sequence": "^2.2.1"
  }
}
